{
  "incremental": true,
  "lineWidth": 80,
  "indentWidth": 2,
  "typescript": {
    "locked": true,
    "spaceSurroundingProperties": false,
    "importDeclaration.spaceSurroundingNamedImports": false,
    "typeLiteral.spaceSurroundingProperties": false,
    "exportDeclaration.spaceSurroundingNamedExports": false,
    "quoteStyle": "preferSingle",
    "module.sortImportDeclarations": "maintain"
  },
  "json": {
    "locked": true
  },
  "includes": [
    "**/*.{ts,tsx,jsx,cjs,mjs,json,cs}"
  ],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/obj/**",
    "**/bin/**",
    "**/dist/**",
    "**/schema.json",
    "**/__generated__/**",
    "**/Migrations/*.cs",
    "**/Data/**",
    "**/*.generated.cs"
  ],
  "roslyn": {
    "locked": true,
    "indentWidth": 4
  },
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.85.1.wasm",
    "https://plugins.dprint.dev/json-0.17.4.wasm",
    "https://plugins.dprint.dev/roslyn-0.17.1.json@9e9bbbd9f0536c779e83e1aacfe4b316bdddd71c269d64468eb8af838ab9a1af"
  ]
}
